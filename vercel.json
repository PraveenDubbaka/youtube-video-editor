{
  "version": 2,
  "builds": [
    {
      "src": "dist/youtube-editor/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(assets/.+)",
      "dest": "/dist/youtube-editor/browser/$1"
    },
    {
      "src": "/(.*\\.css)",
      "dest": "/dist/youtube-editor/browser/$1"
    },
    {
      "src": "/(.*\\.js)",
      "dest": "/dist/youtube-editor/browser/$1"
    },
    {
      "src": "/(.*\\.ico)",
      "dest": "/dist/youtube-editor/browser/$1"
    },
    {
      "src": "/(.*\\.svg)",
      "dest": "/dist/youtube-editor/browser/$1"
    },
    {
      "src": "/(.*\\.jpg)",
      "dest": "/dist/youtube-editor/browser/$1"
    },
    {
      "src": "/(.*\\.png)",
      "dest": "/dist/youtube-editor/browser/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/youtube-editor/browser/index.html"
    }
  ]
}